	local Event = game:GetService("ReplicatedStorage").RemoteEvents.Equip
	Event:FireServer(A_1)
	wait(0.5)
	local A_1 = "The banana"
	local Event = game:GetService("ReplicatedStorage").RemoteEvents.Drop
	Event:FireServer(A_1)
    
wait(0.2)

    end
    else
_G.on = false
        
    end
end)

